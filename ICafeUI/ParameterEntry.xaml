<UserControl x:Class="ICafeUI.ParameterEntry"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ICafeUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border x:Name="Border" Background="White" BorderBrush="Black" BorderThickness="1" MouseRightButtonUp="ParameterEntry_MouseLeftButtonUp">
            <TextBlock x:Name="Label" Text="label" Padding="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center"></TextBlock>
        </Border>
    </Grid>
</UserControl>
